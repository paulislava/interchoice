.flowchart
  overflow hidden
  display flex
  width 100%
  height 100vh
  flex-flow column
  .header
    width 100%
    display flex
    flex-flow row
    justify-content space-between
  .flowchartContent
    width 100%
    height 100%

.sceneName
  color #C4C4C4
  font-weight 500
.sceneActions
  display flex
  flex-flow row
  justify-content space-between